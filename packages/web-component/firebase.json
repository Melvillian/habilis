{
  "hosting": {
    "target": "orbit-js",
    "public": "build",
    "headers": [
      {
        "source": "*",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=300, s-maxage=31536000"
          }
        ]
      }
    ]
  }
}
